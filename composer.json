{
    "name": "chunhei2008/hyperf-translation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "wangyi",
            "email": "chunhei2008@qq.com"
        }
    ],
    "keywords": [
        "php",
        "hyperf"
    ],
    "description": "",
    "autoload": {
        "psr-4": {
            "Chunhei2008\\Hyperf\\Translation\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Chunhei2008\\Hyperf\\Translation\\Tests\\": "tests"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-swoole": ">=4.3",
        "hyperf/utils": "1.0.*",
        "hyperf/framework": "1.0.*",
        "hyperf/di": "1.0.*",
        "psr/container": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "hyperf/testing": "1.0.*",
        "mockery/mockery": "^1.2",
        "phpstan/phpstan": "^0.10.5",
        "swoft/swoole-ide-helper": "dev-master"
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "co-phpunit -c phpunit.xml --colors=always --dev",
        "analyze": "phpstan analyse --memory-limit 300M -l 0 ./src",
        "cs-fix": "php-cs-fixer fix $1"
    },
    "extra": {
        "hyperf": {
            "config": "Chunhei2008\\Hyperf\\Translation\\ConfigProvider"
        }
    }
}
